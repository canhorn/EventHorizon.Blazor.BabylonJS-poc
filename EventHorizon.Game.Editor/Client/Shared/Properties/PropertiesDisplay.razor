@inherits PropertiesDisplayModel
@using EventHorizon.Game.Editor.Zone.Editor.Services.Model

<div class="entity-properties-display">
    @foreach (var prop in DisplayProperties.Values)
    {
        <PropertyDisplay Property="prop"
                         OnChanged="HandlePropertyChanged"
                         OnRemove="OnRemove" />
        <hr class="--accent-color" />
    }
</div>
